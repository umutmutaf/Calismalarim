
@{
    ViewBag.Title = "CarpimTablosu";
}

@for (int i = 0; i < 6; i++)
{
    if (i % 2 == 0)
    {
        <div style="background-color:orange;width:300px;height:150px;float:left;margin:5px;"></div>
    }
    else
    {
        <div style="background-color:yellow;width:300px;height:150px;float:left;margin:5px;"></div>
    }
}

